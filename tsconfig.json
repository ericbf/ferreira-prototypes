{
    "compilerOptions": {
		"declaration": true,
		"inlineSources": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"outFile": "dist/dist.js",
		"sourceMap": true,
		"strictNullChecks": true,
		"suppressImplicitAnyIndexErrors": true,
        "module": "amd",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitUseStrict": false,
        "preserveConstEnums": true,
        "removeComments": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5"
    },
    "exclude": [
        "node_modules",
		"bower_components",
		"dist"
    ],
	"buildOnSave": false,
    "compileOnSave": false,
    "atom": {
        "rewriteTsconfig": false
    }
}
